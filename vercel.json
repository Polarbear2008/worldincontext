{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "outputDirectory": "dist",
  "installCommand": "npm ci",
  "buildCommand": "npm run build",
  "framework": "vite",
  "regions": ["iad1"]
}
